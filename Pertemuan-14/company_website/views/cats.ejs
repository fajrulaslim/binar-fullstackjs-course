<!DOCTYPE html>
<html>
  <head>
    <title>Cats</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%- include('partials/navigation') %>

    <h1>List of Cats</h1>

    <ol>
        <% data.forEach(function(cat){ %>
            <li>Name: <%= cat.name %>, age: <%= cat.age %></li>
        <% }); %>
    </ol>
  </body>
</html>
